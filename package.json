    "coveralls": "^3.0.0",
    "eslint": "^4.9.0",
    "express": "^4.16.2",
    "express-validator": "^5.0.3",
    "morgan": "^1.9.0",
    "nyc": "^11.6.0",
    "validator": "^9.4.1",
    "yamljs": "^0.3.0"
